doctype html
html
   head
        title Hello Student
   body
        table(border=2)
            thead 
                th Name 
                th Email 
                th Phone
                th City 
                th Profession 
                th Industry 
            each val in allemp
                tr
                    td #{val.name}
                    td #{val.email}
                    td #{val.phone}
                    td #{val.city}
                    td #{val.prof}
                    td #{val.indus}