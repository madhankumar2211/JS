doctype html
html
   head
        title Hello Student
   body
        table(border=2)
            thead 
                th Name 
                th Email 
                th Phone
            each val,i in allstud
                tr
                    td 
                        //- a(href="/student/detail/"+i) #{val.name}
                        a(href=`/student/detail/${val.phone}`) #{val.name}

                    td #{val.email}
                    td #{val.phone}
                    